{
  "formatter": {
    "enabled": true,
    "lineWidth": 127
  },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "style": {
        "noNonNullAssertion": "off"
      },
      "suspicious": {
        "noExplicitAny": "warn"
      },
      "complexity": {
        "useSimplifiedLogicExpression": "warn"
      }
    }
  }
}
